<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="es.redmetro.dam2.vo.LineaEstacion" table="t_linea_estacion">
        <id name="linea" type="java.lang.Integer" column="cod_linea">
            <generator class="identity" />
        </id>

        
        <property name="estacion" column="cod_estacion" type="java.lang.Integer"/>
        <property name="ordenM" column="orden" type="java.lang.Integer"/>
        
        
        
    </class>
</hibernate-mapping>